[urgency_low]
    background = "{{ "dunst:low:bg" | hex }}"
    foreground = "{{ "dunst:low:fg" | hex }}"
    frame_color = "{{ "dunst:low:frame" | hex }}"

[urgency_normal]
    background = "{{ "dunst:normal:bg" | hex }}"
    foreground = "{{ "dunst:normal:fg" | hex }}"
    frame_color = "{{ "dunst:normal:frame" | hex }}"

[urgency_critical]
    background = "{{ "dunst:urgent:bg" | hex }}"
    foreground = "{{ "dunst:urgent:fg" | hex }}"
    frame_color = "{{ "dunst:urgent:frame" | hex }}"